{
  "name": "@byline/dashboard",
  "private": false,
  "license": "AGPL",
  "version": "0.1.0",
  "description": "Byline CMS dashboard app",
  "keywords": [
    "cms",
    "headless cms",
    "content management"
  ],
  "homepage": "https://github.com/Byline-CMS/bylinecms.dev",
  "bugs": {
    "url": "https://github.com/Byline-CMS/bylinecms.dev/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Byline-CMS/bylinecms.dev.git",
    "directory": "apps/dashboard"
  },
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:app": "vite",
    "dev:server": "tsx --env-file=.env --watch server/index.ts",
    "build": "run-s build:*",
    "build:server": "tsc -p tsconfig.server.json",
    "build:app": "tsc -p tsconfig.json && vite build",
    "preview": "run-p preview:*",
    "preview:server": "NODE_ENV=production node --env-file=.env ./build/server/index.js",
    "preview:app": "vite preview",
    "clean": "rimraf node_modules dist build .turbo",
    "lint": "biome check --write --unsafe --diagnostic-level=error",
    "test": "LOG_LEVEL=off tsx --env-file=.env --test $(find . -name \"*.test.ts\")",
    "test:one": "LOG_LEVEL=off tsx --no-warnings --env-file=.env --test"
  },
  "dependencies": {
    "@byline/core": "workspace:*",
    "@byline/db-postgres": "workspace:*",
    "@byline/shared": "workspace:*",
    "@byline/uikit": "workspace:*",
    "@fastify/cors": "^11.1.0",
    "@lexical/clipboard": "0.35.0",
    "@lexical/code": "0.35.0",
    "@lexical/hashtag": "0.35.0",
    "@lexical/headless": "0.35.0",
    "@lexical/html": "0.35.0",
    "@lexical/link": "0.35.0",
    "@lexical/list": "0.35.0",
    "@lexical/mark": "0.35.0",
    "@lexical/markdown": "0.35.0",
    "@lexical/overflow": "0.35.0",
    "@lexical/react": "0.35.0",
    "@lexical/rich-text": "0.35.0",
    "@lexical/selection": "0.35.0",
    "@lexical/table": "0.35.0",
    "@lexical/utils": "0.35.0",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-router": "^1.131.36",
    "@tanstack/react-router-devtools": "^1.131.36",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "intl-messageformat": "^10.7.16",
    "jose": "^6.1.0",
    "lexical": "0.35.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-json-view-lite": "^2.5.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.5",
    "zod-form-data": "^3.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/router-plugin": "^1.131.36",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^7.0.1",
    "@types/nodemailer-html-to-text": "^3.1.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "classnames": "^2.5.1",
    "prettier": "^3.6.2",
    "tailwindcss": "4.1.13",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "vite": "^7.1.5"
  }
}
